{% extends "base.html.twig" %}
{% block content %}
<center>
    <div style="margin-bottom: 50px; margin-top: 40px">
        <h1 style="margin-bottom: 20px"> Liste des abonnements Extra</h1>
    <table border="1">
        <tr>
            <th>N_carte_Membre</th>
            <th>nom</th>
            <th>prenom</th>
            <th>duree_abonnement</th>
            <th>pack</th>
            <th>mode_paiement</th>
            <th>validite</th>
            <th>centre interet</th>
            <th>capacit√© physique</th>
            <th align="center" colspan="2">Action</th>

        </tr>
        {% for m in abonnementextra %}
            <tr>
                <td>{{ m.nCartemembre }}</td>
                <td>{{ m.nom }}</td>
                <td>{{ m.prenom }}</td>
                <td>{{ m.dureeAbonnement }}</td>
                <td>{{ m.pack }}</td>
                <td>{{ m.modePaiement }}</td>
                <td>{{ m.validite }}</td>
                <td>{{ m.centreinteret }}</td>
                <td>{{ m.capacitephy }}</td>
                <td><a class="btn btn-dark" href="{{ path('update_e',{'nCartemembre':m.nCartemembre}) }} ">Modifier</a></td>
                <td><a class="btn btn-danger" href="{{ path('delete_e',{'nCartemembre':m.nCartemembre}) }} ">Supprimer</a></td>
            </tr>
        {%endfor %}
    </table>
</center>
{% endblock %}